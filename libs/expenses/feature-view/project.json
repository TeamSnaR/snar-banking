{
  "name": "expenses-feature-view",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/expenses/feature-view/src",
  "prefix": "snarbanking-workspace",
  "tags": ["scope:snarbanking-app", "type:feature"],
  "projectType": "library",
  "targets": {
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/expenses/feature-view/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "libs/expenses/feature-view/**/*.ts",
          "libs/expenses/feature-view/**/*.html"
        ]
      }
    }
  }
}
